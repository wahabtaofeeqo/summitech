{
    "extends": "@tsconfig/node14/tsconfig.json",
    "compilerOptions": {
      "outDir": "build/src",
      "declaration": false,
      "noImplicitAny": false,
      "experimentalDecorators": true,
      "emitDecoratorMetadata": true,
      "strictPropertyInitialization": false
    },
    "include": ["src"],
    "exclude": ["node_modules"]
}